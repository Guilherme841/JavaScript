<!DOCTYPE html>
<html lang="pt-br">
    <%- include('./partials/head.ejs')%>
<body>
    <%- include('./partials/navbar.ejs')%>
    <div class="container">
        <div class="content">
            <% if (users.length > 0) { %> <% users.forEach(user => { %>
            <h3 class="title">
                <%= user.firstName %> <%= user.lastName %>
            </h3>
            <p class="email">
                <%= user.email %>
            </p>
                <% }) %> <% } else { %>
            <p>Não há nenhum usuário!</p>
            <% } %> 
        </div>
    </div>
</body>
</html>